ftgfw-router:
  repo_url: https://github.com/mklnz/ftgfw-router.git

shadowsocks:
  server:
  server_port:
  local: 127.0.0.1
  local_port: 1080
  password:
  timeout: 60
  method: aes-128-cfb
  dns: 8.8.8.8:53

gfwlist2dnsmasq:
  base_url: https://raw.githubusercontent.com/gfwlist/gfwlist/master/gfwlist.txt
  out_file: /opt/ftgfw-router/gfwlist2dnsmasq-rb/dnsmasq.d/gfwlist_rules.conf
  dns_host: 127.0.0.1
  dns_port: 5353
  ipset_name: gfwlist
  extras:
  - google.com
  - google.com.hk
  - google.com.jp
  - google.com.au
